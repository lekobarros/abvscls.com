{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt prepare && vue-tsc -b --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:quick": "playwright test --project='Desktop Chrome 1920x1080' --project='iPhone 14 Pro Max'",
    "test:e2e:desktop": "playwright test --project='Desktop Chrome 1920x1080' --project='Desktop Safari' --project='Desktop Edge'",
    "test:e2e:mobile": "playwright test --project='iPhone 14 Pro Max' --project='Pixel 7'",
    "test:e2e:ui": "playwright test --ui"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/image": "^1.5.0",
    "@nuxt/test-utils": "^3.20.1",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^18.17.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.15",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-mcp-server": "^1.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "happy-dom": "^20.0.11",
    "nuxt": "^4.2.1",
    "postcss": "^8.4.30",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14",
    "vue-eslint-parser": "^10.2.0"
  },
  "dependencies": {
    "@nuxtjs/fontaine": "^0.4.1",
    "@pinia/nuxt": "^0.4.11",
    "@studio-freight/lenis": "^1.0.23",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "gsap": "^3.12.5",
    "lenis": "^1.1.13",
    "motion-v": "^1.5.0",
    "pinia": "^2.1.6",
    "sass": "^1.94.2"
  },
  "overrides": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "estree-walker": "^2.0.2",
    "vue": "latest"
  }
}
